    {
      "cell_type": "code",
      "source": [
        "i=1\n",
        "while i<10:\n",
        "  j=1\n",
        "  while j<10:\n",
        "    print('{}x{}={}\\t'.format(j,i,i*j),end=' ')\n",
        "    j+=1\n",
        "  print()\n",
        "  i+=1"
